{
  "name": "fortguns-shop",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "prebuild": "pnpm dotenv -e .env.local -- pnpm run fetch:all",
    "build": "IS_BUILD=1 next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "next lint",
    "format": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "fetch:all": "pnpm dotenv -e .env.local -- tsx scripts/fetch-all-data.ts",
    "version:patch": "npm version patch && git push --follow-tags",
    "version:minor": "npm version minor && git push --follow-tags",
    "version:major": "npm version major && git push --follow-tags",
    "version:prerelease": "npm version prerelease && git push --follow-tags",
    "version:show": "echo \"Current version: $(node -p \"require('./package.json').version\")\"",
    "release:patch": "pnpm run version:patch && pnpm run build",
    "release:minor": "pnpm run version:minor && pnpm run build",
    "release:major": "pnpm run version:major && pnpm run build"
  },
  "dependencies": {
    "@bprogress/core": "^1.3.4",
    "@bprogress/next": "^3.2.12",
    "@hookform/resolvers": "^5.0.1",
    "@react-google-maps/api": "^2.20.6",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "axios": "^1.8.4",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "eslint-plugin-import": "^2.31.0",
    "html-react-parser": "^5.2.5",
    "html-to-json-parser": "^2.0.1",
    "lodash": "^4.17.21",
    "modern-normalize": "^3.0.1",
    "next": "15.5.4",
    "next-intl": "^4.1.0",
    "next-sitemap": "^4.2.3",
    "p-limit": "^7.1.1",
    "react": "19.1.1",
    "react-cookie-consent": "^9.0.0",
    "react-dom": "19.1.1",
    "react-google-recaptcha-v3": "^1.11.0",
    "react-hook-form": "^7.56.3",
    "schema-dts": "^1.1.5",
    "start": "^5.1.0",
    "swiper": "^11.2.5",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.17.52",
    "@types/react": "19.1.16",
    "@types/react-dom": "19.1.9",
    "autoprefixer": "^10.4.21",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "postcss": "^8.5.4",
    "prettier": "3.4.2",
    "prettier-plugin-css-order": "^2.1.2",
    "sass": "^1.89.0",
    "stylelint": "^16.19.1",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "tsx": "^4.20.3",
    "typescript": "^5"
  },
  "resolutions": {
    "@types/react": "19.1.16",
    "@types/react-dom": "19.1.9"
  },
  "packageManager": "pnpm@9.0.0"
}
